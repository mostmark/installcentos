[OSEv3:children]
masters
etcd
nodes

[OSEv3:vars]
ansible_ssh_user=root
deployment_type=openshift-enterprise
openshift_master_identity_providers=[{'name': 'htpasswd_auth', 'login': 'true', 'challenge': 'true', 'kind': 'HTPasswdPasswordIdentityProvider', 'filename': '/etc/origin/master/htpasswd'}]
containerized=true
openshift_public_hostname=console.openshift.ocp
openshift_master_default_subdomain=apps.console.openshift.ocp
openshift_hosted_metrics_deploy=false
enable_excluders=false

#Disable disk, memory and docker storage checks
openshift_disable_check=disk_availability,memory_availability,docker_storage

[masters]
console.openshift.ocp openshift_schedulable=true

[etcd]
console.openshift.ocp

[nodes]
console.openshift.ocp openshift_node_labels="{'region': 'infra', 'zone': 'default'}" openshift_schedulable=true
